{
  "compile_force_framework": "foundry",
  "exclude_paths": [
    "node_modules",
    "lib",
    "dependencies",
    "test",
    "script"
  ],
  "filter_paths": [
    "src"
  ],
  "exclude_informational": false,
  "exclude_low": false,
  "exclude_medium": false,
  "exclude_high": false,
  "exclude_optimization": false,
  "detectors_to_run": [
    "abiencoderv2-array",
    "arbitrary-send-erc20",
    "arbitrary-send-eth",
    "array-by-reference",
    "assembly",
    "assert-state-change",
    "backdoor",
    "bad-prng",
    "benign-reentrancy",
    "boolean-cst",
    "calls-loop",
    "centralization-risk",
    "checked-send",
    "constant-function",
    "controlled-delegatecall",
    "costly-loop",
    "dead-code",
    "delegatecall-loop",
    "divide-before-multiply",
    "domain-separator-collision",
    "erc20-interface",
    "erc721-interface",
    "events-access",
    "events-maths",
    "external-function",
    "function-init-state",
    "incorrect-equality",
    "incorrect-shift",
    "incorrect-unary",
    "low-level-calls",
    "mapping-deletion",
    "missing-events-access-control",
    "missing-events-arithmetic",
    "missing-zero-check",
    "msgvalue-loop",
    "multiple-constructors",
    "name-reused",
    "protected-vars",
    "public-mappings-nested",
    "reentrancy-benign",
    "reentrancy-eth",
    "reentrancy-events",
    "reentrancy-no-eth",
    "reentrancy-unlimited-gas",
    "reused-constructor",
    "rtlo",
    "shadowing-abstract",
    "shadowing-builtin",
    "shadowing-local",
    "shadowing-state",
    "similar-names",
    "solc-version",
    "storage-array",
    "suicidal",
    "timestamp",
    "too-many-digits",
    "tx-origin",
    "unchecked-lowlevel",
    "unchecked-send",
    "unchecked-transfer",
    "unimplemented-functions",
    "uninitialized-fptr-cst",
    "uninitialized-local",
    "uninitialized-state",
    "uninitialized-storage",
    "unprotected-upgrade",
    "unused-return",
    "unused-state",
    "variable-scope",
    "void-cst",
    "weak-prng",
    "write-after-write"
  ],
  "detectors_to_exclude": [
    "naming-convention",
    "pragma",
    "solc-version",
    "assembly",
    "dead-code",
    "low-level-calls"
  ],
  "hardhat_cache_directory": "./cache",
  "hardhat_artifacts_directory": "./out",
  "foundry_out_directory": "out",
  "foundry_cache_directory": "cache",
  "foundry_compile_all": true,
  "show_ignored_findings": false,
  "json": null,
  "sarif": null,
  "markdown": null,
  "codex": false,
  "codex_contracts": "",
  "codex_model": "text-davinci-003",
  "codex_temperature": 0,
  "codex_max_tokens": 300,
  "codex_log": false,
  "codex_partial_features": false,
  "codex_propmt_file": null,
  "codex_overwrite": true,
  "fail_pedantic": false,
  "fail_low": false,
  "fail_medium": false,
  "fail_high": true,
  "no_fail": false,
  "legacy_ast": false,
  "zip": null,
  "zip_type": "lzma",
  "show_ignored_findings": false,
  "checklist": false,
  "checklist_limit": "",
  "wiki_detectors": false,
  "wiki_printer": false,
  "wiki_slithir": false,
  "list_detectors": false,
  "list_printers": false,
  "include_paths": [],
  "print_all_paths": false,
  "print_summary": true,
  "print_human_summary": true,
  "solc_remaps": [
    "@openzeppelin/=lib/openzeppelin-contracts/contracts/",
    "@openzeppelin-upgradeable/=lib/openzeppelin-contracts-upgradeable/contracts/",
    "solmate/=lib/solmate/src/",
    "forge-std/=lib/forge-std/src/",
    "solady/=lib/solady/src/",
    "@/=src/"
  ]
}